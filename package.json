  },
  "files": [
    "lib",
    "src"
  ],
  "dependencies": {},
  "peerDependencies": {
    "fp-ts": "^2.6.1",
    "io-ts": "^2.2.2"
  },
  "devDependencies": {
    "@types/jest": "25.2.1",
    "@types/node": "13.11.1",
    "fp-ts": "2.6.1",
    "io-ts": "2.2.2",
    "jest": "25.3.0",
    "prettier": "2.0.4",
    "ts-jest": "25.3.1",
    "typescript": "3.9.2"
  }
