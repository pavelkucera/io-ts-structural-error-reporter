{
  "name": "@pavelkucera/io-ts-structural-error-reporter",
  "author": "Pavel Kucera",
  "description": "Experiment: report io-ts errors structurally",
  "license": "MIT",
  "version": "0.0.0",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "prettier": "prettier --config .prettierrc --check src/**/*.ts test/**/*.ts",
