{
  "name": "@pavelkucera/io-ts-structural-error-reporter",
  "author": "Pavel Kucera",
  "description": "Report io-ts errors using the structure of the underlying validators",
  "license": "MIT",
  "version": "0.1.1",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "prettier": "prettier --config .prettierrc --check src/**/*.ts test/**/*.ts",
    "test": "jest",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --noEmit --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pavelkucera/io-ts-structural-error-reporter.git"
  },
  "bugs": {
    "url": "https://github.com/pavelkucera/io-ts-structural-error-reporter/issues"
  },
  "files": [
    "lib",
    "src"
  ],
  "dependencies": {},
  "peerDependencies": {
    "fp-ts": "^2.5.0",
    "io-ts": "^2.0.0"
  },
  "devDependencies": {
    "@types/jest": "26.0.14",
    "@types/node": "13.11.1",
    "fp-ts": "2.5.0",
    "io-ts": "2.0.0",
    "jest": "26.4.2",
    "prettier": "2.0.4",
    "ts-jest": "26.4.1",
    "typescript": "4.0.3"
  },
  "keywords": [
    "io-ts",
    "typescript",
    "runtime"
  ]
}
